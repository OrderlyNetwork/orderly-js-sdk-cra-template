  "dependencies": {
    "@orderly.network/affiliate": "2.6.0",
    "@orderly.network/i18n": "2.6.0",
    "@orderly.network/markets": "2.6.0",
    "@orderly.network/portfolio": "2.6.0",
    "@orderly.network/react-app": "2.6.0",
    "@orderly.network/trading": "2.6.0",
    "@orderly.network/trading-leaderboard": "2.6.0",
    "@orderly.network/trading-rewards": "2.6.0",
    "@orderly.network/types": "2.6.0",
    "@orderly.network/ui": "2.6.0",
    "@orderly.network/ui-scaffold": "2.6.0",
    "@orderly.network/wallet-connector": "2.6.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.119",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "node-polyfill-webpack-plugin": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^7.5.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "craco start",
    "build": "npm run copyLocales && craco build",
    "test": "craco test",
    "eject": "craco eject",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "copyLocales": "node scripts/copyLocales.js",
    "prepare": "husky"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "fs-extra": "^11.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write"
    ]
  }
}